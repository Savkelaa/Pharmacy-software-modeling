<Window x:Class="Pharmacy.Interface.SelectValue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pharmacy.Interface"
        mc:Ignorable="d"
        Title="SelectValue" Height="120" Width="250">
    <Grid>
        <TextBox x:Name="tbQuantity" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Select quantity of selected medicine:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <Button Content="Select" HorizontalAlignment="Left" Margin="144,41,0,0" VerticalAlignment="Top" Width="69" Height="23" Click="Button_Click"/>

    </Grid>
</Window>
